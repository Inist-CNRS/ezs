{
    "name": "ezs-lodex",
    "version": "1.0.0",
    "description": "ezs dedicated statements for Lodex",
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Inist-CNRS/ezs-lodex.git"
    },
    "keywords": [
        "ezs"
    ],
    "author": "Nicolas Thouvenin <nthouvenin@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Inist-CNRS/ezs-lodex/issues"
    },
    "homepage": "https://github.com/Inist-CNRS/ezs-lodex#readme",
    "dependencies": {},
    "main": "./lib/index.js",
    "scripts": {
        "test": "istanbul test ./node_modules/.bin/_mocha -- -R spec",
        "doc": "documentation readme package.json src/* --section=Statements",
        "lint": "eslint --ext=.js ./test/*.js ./src/*.js",
        "build": "babel src --out-dir lib",
        "prepublish": "npm run build",
        "pretest": "npm run build",
        "preversion": "npm run doc",
        "postversion": "git push && git push --tags"
    },
    "devDependencies": {
        "babel-cli": "6.26.0",
        "babel-eslint": "10.0.1",
        "babel-preset-env": "1.7.0",
        "babel-preset-stage-3": "6.24.1",
        "documentation": "8.1.2",
        "eslint": "5.9.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-plugin-import": "2.14.0",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-react": "7.11.1",
        "expect": "23.6.0",
        "ezs": "6.0.0",
        "from": "0.1.7",
        "istanbul": "0.4.5",
        "mocha": "5.2.0"
    },
    "peerDependencies": {
        "ezs": "*"
    }
}
